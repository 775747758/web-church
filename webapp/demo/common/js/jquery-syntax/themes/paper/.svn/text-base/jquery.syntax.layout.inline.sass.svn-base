.syntax-container.syntax-theme-paper
  code.syntax
    display: inline-block
    white-space: no-wrap
    background: #fff
    color: #555
    border: 1px solid #ddd
    -moz-border-radius: .4em
    -webkit-border-radius: .4em
    -o-border-radius: .4em
    -ms-border-radius: .4em
    -khtml-border-radius: .4em
    border-radius: .4em
    padding: 0 .3em
    margin: -1px 0
